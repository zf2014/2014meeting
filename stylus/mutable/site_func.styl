$bg-url(path, domain = $imagesDomian, version = $imagesVersion)
	domain + path + '?v=' + version

// 行高|IE不能居中
$site-lineheight($lh)
	line-height: $lh
	line-height: s('%s\9', $lh+1)


$iconfont()
	font-family: 'seaIconFont'
	// font-size: 1.6em
	-webkit-font-smoothing antialiased
	-webkit-text-stroke-width: 0.2px
	font-style: normal